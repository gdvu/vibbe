{
  "name": "@vibbe/react",
  "version": "2.0.0-alpha.17",
  "description": "Vibbe is a simple, customizable and lightweight component library for building applications in Next.",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "packageManager": "yarn@3.2.0",
  "author": {
    "name": "Moises Machuca Valverde",
    "email": "rmoimv@gmail.com"
  },
  "keywords": [
    "react",
    "next",
    "design",
    "next-component",
    "react-component",
    "vibbe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gdvu/vibbe",
    "directory": "packages/vibbe-base"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn build:cjs && yarn build:esm && yarn build:types && yarn build:packages && yarn build:copy",
    "build:cjs": "babel ./src -x .js,jsx,.ts,.tsx --out-dir ./dist/cjs --env-name 'cjs'",
    "build:esm": "babel ./src -x .js,jsx,.ts,.tsx --out-dir ./dist/esm --env-name 'esm'",
    "build:types": "yarn tsc -p ./tsconfig.build.json -d --emitDeclarationOnly --outDir ./dist/types",
    "build:umd": "NODE_ENV=production rollup -c",
    "build:packages": "node scripts/generate-package.js",
    "build:copy": "node scripts/copy-files.js"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-replace": "4.0.0",
    "@rollup/plugin-typescript": "8.3.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/babel__core": "7",
    "@types/babel__preset-env": "7",
    "@types/jest": "27.4.1",
    "@types/react": "17",
    "@types/react-dom": "17.0.14",
    "@types/rollup-plugin-peer-deps-external": "2",
    "@types/testing-library__jest-dom": "5",
    "babel-loader": "8.2.3",
    "babel-plugin-macros": "3.1.0",
    "colors": "1.4.0",
    "fs-extra": "10.0.1",
    "jest": "27.5.1",
    "react": "17.0.2",
    "react-dom": "18.0.0",
    "rollup": "2.69.0",
    "rollup-plugin-dts": "4.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "styled-jsx": "5.0.2",
    "tiny-glob": "0.2.9",
    "tslib": "2.3.1",
    "typescript": "4.6.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
